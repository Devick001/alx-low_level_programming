#include <stdio.h>
/**
 *main: Entry point
 *
 *Description: Displays sizes of data types in bytes
 *
 *return: 0
*/
int main(void)
{
	char c;
	int d;
	double i;
	floats f;

printf ("size of char is: ", "%c"\n, "size of int is: "%d", "size of double is: " "%i"\n, "size of flots is: " "%f", "c", "d", "i", "f");

        return (0);
}
